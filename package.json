{
  "name": "devfarm-app-starter",
  "scripts": {
    "build": "next build",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon --exec node server.js",
    "dev-debug": "node_modules/.bin/nodemon --exec node-debug server.js",
    "dev:prepare:create": "NODE_ENV=development sequelize db:create",
    "dev:prepare:drop": "NODE_ENV=development sequelize db:drop",
    "dev:prepare:migrate": "NODE_ENV=development sequelize db:migrate",
    "test:prepare:create": "NODE_ENV=test sequelize db:create",
    "test:prepare:drop": "NODE_ENV=test sequelize db:drop",
    "test:prepare:migrate": "NODE_ENV=test sequelize db:migrate",
    "lint": "node_modules/.bin/eslint ./components/**/*.js ./pages/**/*.js ./styles/**/*.js ./graphql/**/*.js ./db/**/*.js --no-ignore",
    "lint-fix": "node_modules/.bin/eslint ./components/**/*.js ./pages/**/*.js ./styles/**/*.js ./graphql/**/*.js ./db/**/*.js --no-ignore --fix",
    "start": "NODE_ENV=production nodemon --exec node server.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "test-integration": "NODE_ENV=test jest --runInBand --env=node --detectOpenHandles",
    "test-watch": "NODE_ENV=test jest --runInBand --watch --env=node --detectOpenHandles"
  },
  "dependencies": {
    "@babel/core": "7",
    "@material-ui/core": "^4.5.0",
    "@material-ui/styles": "^4.5.0",
    "babel-jest": "^24.9.0",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cross-fetch": "^3.0.4",
    "database-cleaner": "^1.3.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-react": "^8.3.0",
    "graphql-sequelize": "^9.4.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "import": "^0.0.6",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.7.5",
    "next": "^9.0.7",
    "next-eslint": "^0.0.4",
    "next-graphql-react": "^4.0.0",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "promise-polyfill": "^8.1.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sequelize": "^5.21.4",
    "uuid": "^7.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^6.8.0",
    "@testing-library/jest-dom": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "1.18.2",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2"
  }
}
