{
  "name": "devfarm-app-starter",
  "scripts": {
    "build": "next build",
    "dev": "node_modules/.bin/nodemon --exec node server.js",
    "dev-debug": "node_modules/.bin/nodemon --exec node-debug server.js",
    "lint": "node_modules/.bin/eslint ./components/**/*.js ./pages/**/*.js ./styles/**/*.js ./graphql/**/*.js ./db/**/*.js --no-ignore",
    "lint-fix": "node_modules/.bin/eslint ./components/**/*.js ./pages/**/*.js ./styles/**/*.js ./graphql/**/*.js ./db/**/*.js --no-ignore --fix",
    "migrate": "./node_modules/.bin/migrate --config ./db/config.js",
    "mongo": "mongo localhost:27017/devfarm_app_development",
    "start": "NODE_ENV=production nodemon --exec node server.js",
    "test": "NODE_ENV=test jest --runInBand",
    "test-integration": "NODE_ENV=test jest --runInBand --env=node",
    "test-watch": "NODE_ENV=test jest --runInBand --watch"
  },
  "dependencies": {
    "@babel/core": "7",
    "@material-ui/core": "^4.5.0",
    "@material-ui/styles": "^4.5.0",
    "babel-jest": "^24.9.0",
    "body-parser": "^1.19.0",
    "cross-fetch": "^3.0.4",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-react": "^8.3.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "import": "^0.0.6",
    "migrate-mongoose": "^4.0.0",
    "mongoose": "^5.7.3",
    "next": "^9.0.7",
    "next-eslint": "^0.0.4",
    "next-graphql-react": "^4.0.0",
    "promise-polyfill": "^8.1.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^6.8.0",
    "@testing-library/jest-dom": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "1.18.2",
    "supertest": "^4.0.2"
  }
}
